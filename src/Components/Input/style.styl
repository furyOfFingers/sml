.container-input
  display flex
  align-items center
  margin-top 20px
  & > input
    width 200px
    border 1px solid black
    padding 5px 10px
    background transparent
    border-radius 30px
    font bold normal normal 18px/17px Roboto, sans-serif
    color rgb(51, 51, 51)
    outline none
  & > span
    margin-left 10px
